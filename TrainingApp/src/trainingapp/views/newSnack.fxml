<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints percentHeight="12.0" />
    <RowConstraints percentHeight="12.0" />
    <RowConstraints percentHeight="12.0" />
      <RowConstraints percentHeight="12.0" />
      <RowConstraints percentHeight="12.0" />
    <RowConstraints percentHeight="12.0" />
      <RowConstraints percentHeight="28.0" vgrow="ALWAYS" />
  </rowConstraints>
   <children>
      <Label text="Menge in g" GridPane.rowIndex="1" />
      <Label text="Kalorien in kcal" GridPane.rowIndex="2" />
      <Label text="Fett in g" GridPane.rowIndex="3" />
      <Label text="Kohlenhydrate in g" GridPane.rowIndex="4" />
      <Label text="Eiweiß in g" GridPane.rowIndex="5" />
      <TextField fx:id="snackMengeTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="snackKalorienTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="snackFettTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="snackKohlenhydrateTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <TextField fx:id="snackEiweissTextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <HBox alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="6">
         <children>
            <Button fx:id="cancelNewSnackButton" mnemonicParsing="false" onAction="#cancelNewSnack" text="Abbrechen">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="addNewSnackButton" mnemonicParsing="false" onAction="#addNewSnack" text="Hinzufügen">
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </opaqueInsets>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <Label text="Name" />
      <TextField fx:id="snackNameTextField" GridPane.columnIndex="1" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
